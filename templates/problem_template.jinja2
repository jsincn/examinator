% Problem template generated from ExamQuestion
\problem{{'{'}}{{ question_title if question_title else '' }}{{'}'}}
{% if question_description_latex %}
{{ question_description_latex | fix_newlines }}
{% endif %}

{% for idx, sub_question in sub_questions %}
\subproblem{{ '*' if sub_question.starred else '' }}{{'{'}}{{ sub_question.available_points }}{{'}'}}
{{ sub_question.question_text_latex | fix_newlines }}

\begin{solutionbox}[2cm]
{{ sub_question.question_answer_latex | fix_newlines }}
\end{solutionbox}

{% if not loop.last %}
{% endif %}
{% endfor %}
